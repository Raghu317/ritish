pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('artifactory') {
            steps {
                echo 'push artifacts'
            }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}